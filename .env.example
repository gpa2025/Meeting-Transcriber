# AWS Credentials (required for AWS Transcribe and Bedrock)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-bucket-name

# AWS Bedrock Settings
BEDROCK_MODEL_ID=anthropic.claude-v2
# Other options: amazon.titan-text-express-v1, anthropic.claude-instant-v1, anthropic.claude-3-sonnet-20240229-v1:0

# AI Model Settings
MODEL_TEMPERATURE=0.7
MAX_TOKENS=4096
SYSTEM_PROMPT="You are an AI assistant that creates detailed meeting notes from transcripts."

# Transcription Settings
TRANSCRIBE_LANGUAGE_CODE=en-US
ENABLE_SPEAKER_DIARIZATION=true
MAX_SPEAKER_LABELS=10